    "@typescript-eslint/eslint-plugin": "^4.13.0",
    "@typescript-eslint/parser": "^4.13.0",
    "babel-preset-power-assert": "^3.0.0",
    "eslint": "^7.17.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-expect-type": "^0.0.5",
    "espower-typescript": "^9.0.2",
    "jest": "^26.6.3",
    "power-assert": "^1.6.1",
    "prettier": "2.2.1",
